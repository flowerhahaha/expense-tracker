{{#if success_msg}}
<div class="alert alert-dismissible alert-primary">
  <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
  {{success_msg}}
</div>
{{/if}}

{{#if warning_msg}}
<div class="alert alert-dismissible alert-warning">
  <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
  {{warning_msg}}
</div>
{{/if}}

{{#if errors}}
  {{#each errors}}
  <div class="alert alert-dismissible alert-warning">
    <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
    {{this.message}}
  </div>
  {{/each}}
{{/if}}